{
  "name": "channeljs",
  "version": "1.0.7",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "types": "lib/index.d.ts",
  "repository": "https://github.com/fabiankachlock/channeljs",
  "author": "Fabian Kachlock",
  "license": "MIT",
  "scripts": {
    "run": "ts-node --files --project tsconfig.json example",
    "run:watch": "nodemon --watch src --exec 'yarn run' -e ts",
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watch",
    "test:coverage": "jest --config jestconfig.json --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "lint-staged": {
    "src/**/!(*test).ts": [
      "yarn lint"
    ],
    "src/**/*.ts": [
      "yarn format"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node-schedule": "^1.3.2",
    "@types/uuid": "^8.3.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  }
}
